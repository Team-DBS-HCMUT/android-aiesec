<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aiesec">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Aiesec">
        <activity android:name=".JoinProgram"></activity>
        <activity android:name=".AddCompany" />
        <activity android:name=".AddPerson" />
        <activity android:name=".AddStudent" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewStudentDetail" />
        <activity android:name=".ViewMemberDetail" />
        <activity android:name=".AddEvent" />
        <activity android:name=".AddProgram" />
        <activity android:name=".ViewProgramDetail" />
        <activity android:name=".MainScreen" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>